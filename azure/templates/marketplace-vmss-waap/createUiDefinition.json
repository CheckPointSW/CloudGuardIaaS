            "options": {
              "hideConfirmation": false
            },
            "visible": "[and(equals(steps('https').chooseVault, 'newVault'), lessOrEquals(5, coalesce(steps('https').numberOfCerts, int('0'))))]"
          }
        ]
      }
    ],
    "outputs": {
